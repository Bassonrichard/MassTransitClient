{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:5341" }
      }
    ]
  },
  "RabbitMQ": {
    "Host": "localhost",
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "MassTransit": {
    "intervalLimit": 3,
    "initialInterval": 3,
    "intervalIncrement": 2,
    "trackingPeriod": 60,
    "tripThreshold": 15,
    "resetInterval": 60,
    "activeThreshold": 10
  }
}
